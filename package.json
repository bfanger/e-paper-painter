{
  "name": "paint",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack && node src-server/bootstrap",
    "dev": "concurrently --kill-others 'npm run dev:server' 'npm run dev:webapp'",
    "dev:server": "pm2-dev src-server/bootstrap.js",
    "dev:webapp": "webpack-dev-server --inline --host=0.0.0.0",
    "build": "webpack",
    "lint": "eslint --ext .js,.vue src"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "Safari >= 9"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "vue",
      "only-warn"
    ],
    "rules": {
      "no-console": [
        0
      ]
    }
  },
  "dependencies": {
    "@std/esm": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "express": "^4.16.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "image-data-uri": "^1.0.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "vue": "^2.5.13",
    "vue-loader": "^13.6.0",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
